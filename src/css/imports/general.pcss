:root {
  --blue: #14a9ff;
  --dark: #171a1c;
  --white: #ffffff;
}

html {
  width: 100vw;
  min-height: 100vh;
}
body {
  width: 100%;
  height: 100%;
  background-color: var(--dark);
  color: var(--white);
}

.wrapper {
  width: 100%;
  padding: 0;
  margin: 0;
  min-height: 100%;
  position: relative;
}
